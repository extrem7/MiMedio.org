{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "2.2",
        "butschster/meta-tags": "^1.6.8",
        "coderello/laravel-shared-data": "^2.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "devmarketer/easynav": "^1.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "inani/larapoll": "master",
        "jorenvanhocht/laravel-share": "^3.2",
        "laravel/framework": "^7.0",
        "laravel/scout": "^8.0",
        "laravel/socialite": "^4.3",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "mariuzzo/laravel-js-localization": "^1.6",
        "pusher/pusher-php-server": "^4.1",
        "qcod/laravel-app-settings": "^1.2",
        "socialiteproviders/instagram": "^4.1",
        "socialiteproviders/instagram-basic": "^4.1",
        "spatie/laravel-medialibrary": "^7.0.0",
        "staudenmeir/eloquent-eager-limit": "^1.0",
        "tightenco/ziggy": "^0.9.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}
